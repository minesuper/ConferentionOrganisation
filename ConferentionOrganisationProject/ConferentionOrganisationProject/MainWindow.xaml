<Window x:Class="ConferentionOrganisationProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConferentionOrganisationProject"
        mc:Ignorable="d"
        Title="{Binding ElementName=MainFrame, Path=Content.Title}" Height="700" Width="1100"
        MinHeight="700" MinWidth="1100" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Организация конференций"
               HorizontalAlignment="Center" VerticalAlignment="Center"
               FontSize="24" FontFamily="Verdana"></Label>
        <Frame x:Name="MainFrame" Grid.Row="1"
               NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
